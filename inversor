#include <iostream>
#include <cmath>

using namespace std;

int main()
{
    int d=1,n,NdeAlgarismos=0,resto,nf=0,ga,p,lugar=0,negativador=0,m;
    cin>>n;
    if(n==0){
        cout<<"insira um numero vÃ¡lido"<<endl;
        return 0;
    }
    if(n<0){
        n=n*(-1);
        negativador=1;
    }
    while(d<=n){
        NdeAlgarismos=NdeAlgarismos+1;
        d=d*10;
    }
    p=NdeAlgarismos;
    m=n;
    while(p>0){
    if(p>1 && p<NdeAlgarismos){
            resto=m%(10^(p-1));
    ga=(m-resto)/(10^(p-1));
    lugar=NdeAlgarismos-p;
    nf=nf+(ga*(10^lugar));
    p=p-1;
    m=resto;
    }
    if(p==NdeAlgarismos && NdeAlgarismos!=1){
        resto=m%(10^(p-1));
        ga=(m-resto)/(10^(p-1));
        nf=ga;
        p=p-1;
        m=resto;
    }
    if(p==1){
        if(NdeAlgarismos==1){
                nf=m;
                p=p-1;
        }
        if(NdeAlgarismos>1){
                nf=nf+(m*(10^(NdeAlgarismos-1)));
        p=p-1;
    }}
    }
    if(negativador==1){
        nf=nf*(-1);
    }
    cout<<nf<<endl;
    }
