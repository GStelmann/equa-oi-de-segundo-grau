#include <iostream>
#include <cmath>

using namespace std;

int main()
{
    int d=1,n,k=0,g,nf,ga,p,f;
    cin>>n;
    while(d<=n){
        k=k+1;
        d=d*10;
    }
    p=k;
    while(p>0){
    g=n%10^(p-1);
    ga=(n-g)/(10^(p-1));
    f=k-p+1;
    nf=nf+ga*(10^f);
    p=p-1;
    n=g;
    }
    cout<<nf;
    }
